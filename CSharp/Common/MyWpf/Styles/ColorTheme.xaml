<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="BackgroundDarkBrush"
                     Color="#AFE5FF"></SolidColorBrush>
    <SolidColorBrush x:Key="BackgroundDarkDarkBrush"
                      Color="#0032D8"></SolidColorBrush>
    <SolidColorBrush x:Key="MyBorderBrush"
                     Color="DarkGray"></SolidColorBrush>
    <SolidColorBrush x:Key="MyMouseOver"
                     Color="GhostWhite" />
    <SolidColorBrush x:Key="MyDisableTextBrush"
                     Color="Gray" />
    <SolidColorBrush x:Key="MyDisableBorderBrush"
                     Color="LightGray" />

</ResourceDictionary>