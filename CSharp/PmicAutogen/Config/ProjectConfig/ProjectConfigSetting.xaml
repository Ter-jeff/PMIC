<Window x:Class="PmicAutogen.Config.ProjectConfig.ProjectConfigSetting"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ProjectConfigSetting" Height="360" Width="220" WindowStartupLocation="CenterOwner"
        WindowStyle="ToolWindow">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="34" />
        </Grid.RowDefinitions>
        <Grid Grid.Row="0">
            <ScrollViewer Name="SrVmain" HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Auto">
                <Grid Name="GridMain" />
            </ScrollViewer>
        </Grid>
        <Grid Grid.Row="1">
            <Button Name="BtnOk" Content="OK" Margin="0,0,90,0" Height="23" Width="70" VerticalAlignment="Center"
                    HorizontalAlignment="Right" Click="BtnOk_Click" />
            <Button Name="BtnCancel" Content="Cancel" Margin="0,0,10,0" Height="23" Width="70"
                    VerticalAlignment="Center" HorizontalAlignment="Right" Click="BtnCancel_Click" />
        </Grid>
    </Grid>
</Window>